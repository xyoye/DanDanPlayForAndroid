<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xyoye.user_component">

    <application
        android:name="com.xyoye.common_component.base.app.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activities.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activities.login.LoginActivity"/>
        <activity android:name=".ui.activities.register.RegisterActivity"/>
        <activity android:name=".ui.activities.forgot.ForgotActivity"/>
        <activity android:name=".ui.activities.user_info.UserInfoActivity"/>
        <activity android:name=".ui.activities.setting_player.SettingPlayerActivity"/>
    </application>

</manifest>